---
  author: "Tom Roberts <tom@pacify.com>"
  class: "Programming"
  desc: "Structure for programming information"
  methods: 
    - 
      desc: "run code snippet"
      method: "eval_code"
      params: 
        - 
          desc: "the code string to be run"
          type: "string&"
          var: "code"
      return: "int"
      virtual: true
  properties: 
    - 
      desc: "a string with language name"
      type: "string"
      var: "language"
    - 
      desc: "an integer rating for the language"
      type: "int"
      var: "rating"
  subclasses: []
  super: []
